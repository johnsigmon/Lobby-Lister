<!DOCTYPE html>
<html>
  <head><% include ../partials/head %>

  </head>
    <div class="header center">
      <body >
      <nav><% include ../partials/navigation %></nav>

        <div class="container">
        <h3>D3 Graphic</h3>
        <section id="chart">

        </section>
        <div class="row">
          <div class="col s6 offset-s3">
          <%if(companies.length){ %>
          <% companies.forEach( function(companies){  %>
          <div class="card grey darken-1 ">
          <div class="card-content white-text ">
          <span class="card-title"><%=companies.client_name%></span>
            <p>$
              <strong><%=companies.amount%></strong>
              <br>
              <%=companies.client_id%>
              <br>
              <%=companies.year%>
            </p>
            <p>Transaction ID <br>
                <%= %></p>
          </div>
          <div class="card-action">
            <form action='search/details' method= 'get' class="col s12" name=<%=companies.id%>>
            <input type="hidden" name="id" value=<%=companies.id%> />
            <button type="submit" class='blue darken'><%=companies.id%></button>
            </form>
            <button class='saveBill blue darken'>Save</button>
          </div>
          </div>
          <% }) %>
          </div>
          </div>
          <% }else{ %>
          <li>No results</li>
          <% } %>
        </div>
          <script type="text/javascript" src="/js/script.js"></script>
  <script src='https://cdnjs.cloudflare.com/ajax/libs/d3/4.2.7/d3.min.js'></script>
</body>
</div>
</html>
